.full-screen-header .header-details .header-details-owner a { color: rgb(234, 76, 137); }

.full-screen-header .header-details .header-details-owner a.back { display: block; margin: 8px 0px 0px; text-align: center; }

.full-screen-header .header-details .header-details-owner span.meta { font-size: 12px; font-weight: normal; line-height: 1.3; }

.full-screen-header .header-details .header-details-owner span.views { margin: 0px 0px 0px 10px; padding: 0px 0px 0px 18px; background: url("/assets/attachments/icon-attach-views-1b7a54c65ebccedd787912c3519f618f1867192a75aadfac45af136d5125e282.png") 0px 50% / 14px 10px no-repeat; }

.full-screen-header .header-details .header-details-owner h1 { font-family: "Haas Grot Text R Web", "Helvetica Neue", Helvetica, Arial, sans-serif; font-size: 14px; line-height: 20px; font-weight: 500; }

@media only screen and (min-width: 960px) {
  .full-screen-header .header-details .header-details-owner h1 { font-family: "Haas Grot Text R Web", "Helvetica Neue", Helvetica, Arial, sans-serif; font-size: 20px; font-weight: 500; line-height: 29px; }
}

.full-screen-header .header-details .header-details-owner h1 a { color: rgb(13, 12, 34); }

.full-screen-header .header-details .header-details-owner h1 a:hover { color: rgb(231, 231, 233); }

.full-screen-header .header-details .header-details-owner h1 a img { margin: 0px 8px 0px 0px; float: left; }

.full-screen-header .header-thumbnails { display: flex; position: relative; flex-grow: 1; justify-content: flex-end; padding: 0px; overflow-x: hidden; }

.full-screen-header .header-thumbnails .thumbnails-list { box-sizing: border-box; width: 100%; margin: 0px; padding: 10px 4px; font-size: 0px; text-align: center; white-space: nowrap; }

.full-screen-header .header-thumbnails .thumbnails-list::-webkit-scrollbar { display: none; }

.full-screen-header .header-thumbnails .thumbnails-list::after, .full-screen-header .header-thumbnails .thumbnails-list::before { content: ""; position: absolute; top: 0px; width: 30px; height: 100%; transition: opacity 0.2s ease 0s; opacity: 0; pointer-events: none; }

.full-screen-header .header-thumbnails .thumbnails-list.scrollable { overflow: scroll hidden; }

.full-screen-header .header-thumbnails .thumbnails-list.scrolled-left::before { left: 0px; opacity: 1; background: linear-gradient(269.26deg, rgba(255, 255, 255, 0) -6.1%, rgb(255, 255, 255) 100%); }

.full-screen-header .header-thumbnails .thumbnails-list.scrolled-right::after { right: 0px; opacity: 1; background: linear-gradient(272deg, rgb(255, 255, 255) -6.1%, rgba(255, 255, 255, 0) 100%); }

.full-screen-header .header-thumbnails .thumbnails-list li { display: inline-block; vertical-align: top; }

.full-screen-header .header-thumbnails .thumbnails-list li:not(:first-child) { margin-left: 14px; }

.full-screen-header .header-thumbnails .thumbnails-list li.screenshot { margin: 0px 6px 0px 0px; padding: 0px 20px 0px 0px; border-right: 1px solid rgba(255, 255, 255, 0.2); }

.full-screen-header .header-thumbnails .thumbnails-list li a { display: block; width: 60px; height: 45px; overflow: hidden; border-radius: 4px; background: rgb(231, 231, 233); }

@media only screen and (min-width: 960px) {
  .full-screen-header .header-thumbnails .thumbnails-list li a { width: 80px; height: 60px; }
}

.full-screen-header .header-thumbnails .thumbnails-list li a:hover { box-shadow: rgba(255, 255, 255, 0.4) 0px 0px 0px 4px; }

.full-screen-header .header-thumbnails .thumbnails-list li a img { width: 100%; height: auto; }

.full-screen-header .header-thumbnails .thumbnails-list li.active a { box-shadow: rgb(255, 255, 255) 0px 0px 0px 2px, rgb(234, 76, 137) 0px 0px 0px 4px; }

.full-screen-header .close-attachments a { display: flex; position: absolute; top: 8px; right: 0px; align-items: center; justify-content: center; width: 48px; height: 48px; border-radius: 8px; background: rgb(243, 243, 244); color: rgb(13, 12, 34); }

.full-screen-header .close-attachments a:hover { background: rgb(231, 231, 233); }

.full-screen-header .close-attachments a svg { width: 14px; height: 14px; color: rgb(13, 12, 34); }

.full-screen-header.has-close .header-thumbnails { right: 60px; }

@media only screen and (min-width: 960px) {
  .full-screen-header.has-close .header-thumbnails { right: 80px; }
}

.full-screen-header.has-close.is-full .header-thumbnails { right: 0px; }

